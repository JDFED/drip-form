{
	"name": "@jdfed/ajv",
	"version": "0.8.2",
	"description": "drip ajv核心包",
	"keywords": [
		"ajv"
	],
	"author": "JDFED",
	"homepage": "https://github.com/JDFED/drip-form/",
	"license": "MIT",
	"main": "dist/cjs/index.js",
	"typings": "dist/esm/index.d.ts",
	"module": "dist/esm/index.js",
	"distExports": {
		"dist": "./dist",
		"build-info": "./build-info",
		"format": [
			"esm",
			"cjs"
		]
