		"url": "https://github.com/JDFED/drip-form/"
	},
	"dependencies": {
		"@jdfed/drip-form-plugin-formats": "^0.8.2",
		"@jdfed/drip-form-plugin-keywords": "^0.8.2",
		"ajv": "^8.6.2",
		"ajv-errors": "^3.0.0",
		"ajv-formats": "^2.1.0",
		"ajv-formats-draft2019": "^1.4.3",
		"ajv-keywords": "^5.0.0"
	}
